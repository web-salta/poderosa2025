
@Html.Partial("~/Views/Shared/_head.cshtml")
<!-- METAS -->
<title>Publicaciones no Periódicas | PODEROSA</title>
<!-- Basic HTML Meta Tags -->
<meta name='title' content='Publicaciones no periódicas | PODEROSA' />
<meta name='description' content='Publicaciones no Periódicas' />
<meta name='url' content='http://www.poderosa.com.pe/publicaciones/bpublicaciones-no-periodicas' />
<meta name='keywords' content='Publicaciones no periódicas, pataz, poderosa' />
<!-- OpenGraph Meta Tags -->
<meta property='og:type' content='website' />
<meta property='og:title' content='Publicaciones no periódicas | PODEROSA' />
<meta property='og:description' content='Creemos que la participación activa e informada de la población es vital para fortalecer la gobernabilidad y trabajar juntos para alcanzar el desarrollo local.' />
<meta property='og:url' content='http://www.poderosa.com.pe/publicaciones/publicaciones-no-periodicas' />

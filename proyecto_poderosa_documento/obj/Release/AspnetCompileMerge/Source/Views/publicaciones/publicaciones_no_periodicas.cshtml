</head>
<body>
    @Html.Partial("~/Views/Shared/_header.cshtml")

    <main>
        <section class="bg-dorado">
            <div class="banner-interno banner-interno--solo-texto">
                <div class="container   banner-interno__caja-texto">
                    <h1 class="banner-interno__h">PUBLICACIONES NO PERIÓDICAS</h1>
                </div>
            </div>
        </section>

        <section class="my-seccion publicaciones">
            <div class="container">
                <nav>
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">PUBLICACIONES</li>
                        <li class="breadcrumb-item active">PUBLICACIONES NO PERIÓDICAS</li>
                    </ol>
                </nav>

                <div class="publicaciones_pagination row">
                    <div class="col-6 col-sm-4 col-md-3 col-lg-2    mb-4">
                        <a target="_blank" href="~/Content/descargas/publicaciones-no-periodicas/afiche-de-buenas-practicas_g.jpg">
                            <article class="caja-lista">
                                <h3 class="caja-lista__h">Afiche de <br />Buenas Prácticas</h3>
                                <div class="caja-lista__caja-img    p-4    zoom">
                                    <img class="caja-lista__img    zoom__img" src="~/Content/descargas/publicaciones-no-periodicas/afiche-de-buenas-practicas.jpg" />
                                </div>
                                <div class="text-right mt-2">
                                    <span class="boton-icono boton-icono--sm boton-icono--dorado">
                                        <img class="boton-icono__img svg" src="~/Content/img/web/ico-flecha-descargar.svg" alt="ico">
                                    </span>
                                </div>
                            </article>
                        </a>
                    </div>

                    <div class="col-6 col-sm-4 col-md-3 col-lg-2    mb-4">
                        <a target="_blank" href="~/Content/descargas/publicaciones-no-periodicas/triptico-energia-electrica.pdf">
                            <article class="caja-lista">
                                <h3 class="caja-lista__h">Tríptico <br />Energía Eléctrica</h3>
                                <div class="caja-lista__caja-img    p-4    zoom">
                                    <img class="caja-lista__img    zoom__img" src="~/Content/descargas/publicaciones-no-periodicas/triptico-energia-electrica.jpg" />
                                </div>
                                <div class="text-right mt-2">
                                    <span class="boton-icono boton-icono--sm boton-icono--dorado">
                                        <img class="boton-icono__img svg" src="~/Content/img/web/ico-flecha-descargar.svg" alt="ico">
                                    </span>
                                </div>
                            </article>
                        </a>
                    </div>


                </div>
            </div>
        </section>

    </main>
    @Html.Partial("~/Views/Shared/_footer.cshtml")

</body>
@Html.Partial("~/Views/Shared/_foot.cshtml")

<!-- SCRIPTS -->
<script type="text/javascript" src="~/Content/js/pagination/jQuery.paginate.js"></script>
<script>
    $('.publicaciones_pagination').paginate({
        items_per_page: 18
    });
</script>
